<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="xxd.nju.homework.model.UserBean" table="user">
		<id name="userID" column="userID" type="java.lang.String" />
		<property name="username" column="username" type="java.lang.String" />
		<property name="password" column="password" type="java.lang.String" />
		<property name="cardNumber" column="cardNumber" type="java.lang.String" />
		<property name="position" column="position" type="java.lang.String" />
	</class>
</hibernate-mapping>